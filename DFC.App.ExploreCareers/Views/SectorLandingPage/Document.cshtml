@model DFC.App.ExploreCareers.ViewModels.SectorLandingPage.DocumentViewModel
@{
    Layout = "_LocalLayout";
    if (!string.IsNullOrWhiteSpace(Model.Head?.Title))
    {
        ViewData["Title"] = Model.Head.Title;
    }
}

<partial name="Breadcrumb" model=@Model.Breadcrumb />
<partial name="BodyTop" />

<div class="govuk-width-container">
    <div id="compuiShell-ErrorSummary" class="govuk-error-summary dfc-composite-shell-hide" aria-labelledby="compuiShell-error-summary-title" role="alert" tabindex="-1" data-module="govuk-error-summary">
        <h2 class="govuk-error-summary__title" id="compuiShell-error-summary-title">
            Sector landing page
        </h2>
        <div class="govuk-error-summary__body">
            <ul class="govuk-list govuk-error-summary__list">
            </ul>
        </div>
    </div>
</div>

<div class="govuk-main-wrapper">
    <partial name="Body"/>
</div>









